<Window x:Class="Interface.Loading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="Loading" Height="250" Width="350">
    <Grid>
        <Label x:Name="Info" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,50,0,0" FontSize="25" Height="40" FontWeight="Bold" Foreground="Black"/>
        <Label x:Name="Wait" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,100,0,0" FontSize="25" Height="40" FontWeight="Bold" Foreground="Black"/>
    </Grid>
</Window>
